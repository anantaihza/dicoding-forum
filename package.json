{
  "name": "dicoding-forum",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "test:reducers": "vitest reducers-test --watch=false --reporter=verbose",
    "test:thunks": "vitest thunks-test --watch=false --reporter=verbose",
    "test:components": "vitest components-test --watch=false --reporter=verbose",
    "test": "vitest --watch=false",
    "test:e2e": "cypress run",
    "test:all": "concurrently -g \"npm run test:e2e\" \"npm run test\"",
    "open:e2e": "npx cypress open",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "ci:test": "vitest --no-watch && start-server-and-test dev http-get://localhost:5173 test:e2e"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.2",
    "html-react-parser": "^5.1.9",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-onboarding": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "@storybook/react-vite": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "cypress": "^13.8.0",
    "daisyui": "^4.7.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.36",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3",
    "storybook": "^8.0.9",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.6",
    "vitest": "^1.4.0"
  }
}
